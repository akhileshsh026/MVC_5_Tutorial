@model YouTubeCOmplete.Models.Employee

@{
    ViewBag.Title = "Employee";
}

<h2>Employee Details</h2>

<table style="font-family:Arial">
    <tr>
        <td>Employee Id</td>
        
    </tr>
    <tr>
        <td>Employee Name</td>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <td>Employee Age</td>
        <td>@Model.age</td>
    </tr>
    <tr>
        <td>Employee City</td>
        <td>@Model.city</td>
    </tr>

</table>

